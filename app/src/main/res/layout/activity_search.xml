<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/selsct_scrollview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="none"
        android:scrollbars="none"
        android:fillViewport="true"
        android:overScrollMode="never"
        android:background="@mipmap/bg_main">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/marginBottom2"
            android:paddingTop="@dimen/marginBottom2"
            android:paddingLeft="@dimen/marginLeft1"
            android:paddingRight="@dimen/marginLeft1"
            android:background="@android:color/transparent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/chaobiaolr_label_cebenhao"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_cebenhao"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/chaobiaolr_label_customer_code"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_customer_code"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/text_yonghum"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_huming"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/text_yonghudz"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_address"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/hotline_label_phone"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_phone"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:inputType="number"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/datachange_label_jianhao"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_jianhao"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/text_biaohao"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_biaohao"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/label_koujing"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_koujing_begin"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:maxLength="8"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="â€”"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_koujing_end"
                    android:layout_height="@dimen/height8"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:maxLength="8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/label_qianfeibs"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_qianfeibs"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:inputType="number"
                    android:maxLength="8"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/label_qianfeije"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <EditText
                    android:id="@+id/ed_qianfeije"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:inputType="number"
                    android:maxLength="8"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="@dimen/width1"
                    android:layout_height="@dimen/height8"
                    android:gravity="center_vertical"
                    android:text="@string/label_huanbianrqdy"
                    android:textColor="@color/color8"
                    android:textSize="@dimen/text_size_xxlarge" />

                <TextView
                    android:id="@+id/tv_huanbianrq"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/height8"
                    android:textColor="@color/color3"
                    android:textSize="@dimen/text_size_xxlarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:gravity="center"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/btn_select"
                    style="@style/Button8"
                    android:text="@string/button_label_select" />

                <Button
                    android:id="@+id/btn_reset"
                    style="@style/Button8"
                    android:layout_marginLeft="@dimen/marginLeft1"
                    android:text="@string/button_label_reset" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
